index e94e02b..67a5045 100644
--- a/arg.h
+++ b/arg.h
@@ -10,7 +10,7 @@ extern char *argv0;
 
 /* use main(int argc, char *argv[]) */
 #define ARGBEGIN	for (argv0 = *argv, argv++, argc--;\
